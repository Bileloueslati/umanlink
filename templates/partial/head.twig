<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!-->
	<html
	class="no-js no-svg" {{ site.language_attributes }}> <!--<![endif]-->
	<head>
		<meta charset="{{ site.charset }}"/>
		<meta name="description" content="{{ site.description }}">
		{% for item in assets.css %}
			<link rel="stylesheet" href="{{item}}" type="text/css"/>
		{% endfor %}
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="author" href="{{ site.theme.link }}/humans.txt"/>
		<link rel="profile" href="http://gmpg.org/xfn/11">
		<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="{{theme.uri}}/public/img/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="{{theme.uri}}/public/img/favicon-16x16.png">
		{% for item in assets.js %}

			<script src="{{item}}" defer></script>

		{% endfor %}
		{{function('wp_head')}}
	</head>
